{
  "name": "@bsonntag/react-tabs",
  "version": "0.0.1",
  "description": "Flexible tab components for react",
  "repository": "git@github.com:bsonntag/react-tabs.git",
  "license": "MIT",
  "author": "Benjamim Sonntag <benjamimsonntag@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "bundle": "rm -rf dist && babel src --out-dir dist --ignore *.test.js",
    "bundle:watch": "babel src --out-dir dist --watch --ignore *.test.js",
    "test": "jest",
    "test:watch": "jest --watch --notify",
    "version": "npm run bundle && git add dist"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "lcov",
      "text"
    ],
    "setupTestFrameworkScriptFile": "./jest-setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "/node_modules/"
    ]
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^5.11.1",
    "eslint-config-seegno": "^11.0.1",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-testing-library": "^5.4.2"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  }
}
